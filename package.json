{
	"name": "meet",
	"displayName": "Google Meet",
	"description": "Start a new Google Meet directly from VS Code",
	"publisher": "geekanant",
	"version": "0.1.2",
	"icon": "icon.png",
	"engines": {
		"vscode": "^1.0.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/geekanant/Google-Meet-VS-Code-Extension"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:meet.startMeet"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "meet.startMeet",
				"title": "Google Meet"
			}
		],
		"keybindings": [
			{
				"key": "ctrl+alt+g",
				"command": "meet.startMeet",
				"mac": "ctrl+alt+g"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.55.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.19.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	},
	"dependencies": {}
}
